# MyInventory AI Environment Variables

# Database connection (required)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/myinventory

# OpenAI API key for image analysis (required)
OPENAI_API_KEY=sk-...

# Cloudflare Tunnel (optional)
# Set to "true" to auto-start tunnel with pnpm dev:mobile
# Can also use CLI flags: --tunnel or --no-tunnel
ENABLE_TUNNEL=false
